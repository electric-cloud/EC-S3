<plugin>
  <key>@PLUGIN_KEY@</key>
  <version>@PLUGIN_VERSION@</version>
  <label>@PLUGIN_KEY@</label>
  <description>Integration for Amazon S3</description>
  <configure>configurations.xml</configure>
  <help>@PLUGIN_KEY@_help.xml</help>
  <author>CloudBees</author>
  <authorUrl>http://www.electric-cloud.com/support</authorUrl>
  <category>Resource Management</category>
  <ecSupportLevel>10</ecSupportLevel>
  <commander-version min="5.4"/>
  <components>
    <component name="ConfigurationManagement">
      <javascript>war/ecplugins.s3.ConfigurationManagement/ecplugins.s3.ConfigurationManagement.nocache.js</javascript>
    </component>
  </components>
  <hasAdoc>true</hasAdoc>
  <catalogLabel>Amazon Simple Storage Service</catalogLabel>
  <detailedDescription>Amazon Simple Storage Service (S3) is storage for the Internet. It is designed to make web-scale computing easier for developers. This plugin uses the Amazon S3 application programming interface (API) that lets developers choose where their objects are stored on the Amazon cloud.</detailedDescription>
  <hasAdoc>true</hasAdoc>
</plugin>
